      <Label alignment="CENTER" layoutX="670.0" layoutY="23.0" prefHeight="20.0" prefWidth="100.0" text="Użytkownicy" textAlignment="CENTER" />
      <Label layoutX="50.0" layoutY="483.0" prefHeight="17.0" prefWidth="100.0" text="Wiadomość: " />
      <Button fx:id="sendButtonChatStage" layoutX="553.0" layoutY="513.0" mnemonicParsing="false" onAction="#sendMessage" text="Wyślij" />
      <TextField fx:id="messageChatStage" alignment="TOP_LEFT" layoutX="50.0" layoutY="501.0" prefHeight="50.0" prefWidth="450.0" />
